<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.sjassistant" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".activity.BaseActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".activity.ServerConfigActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".activity.ForgetPasswordActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".activity.MainActivity"
            android:screenOrientation="portrait"
            >
        </activity>
        <activity android:name=".activity.SettingActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity android:name=".activity.NoDisturbActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity android:name=".activity.PromptActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".activity.HelpActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name=".activity.ReportActivity">
        </activity>
        <activity android:name=".activity.ChangePwdActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".activity.LoginActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
